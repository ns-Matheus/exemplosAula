.menu {
    -webkit-box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.75);
    -moz-box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.75);
    box-shadow: 0px 0px 5px 0px rgba(0,0,0,0.75);

    width: 80%;
    height: 80px;
    background-color: #fff;
    position: fixed;
    top: 45px;
    left: 10%;
    -webkit-border-radius: 35px;
    -moz-border-radius: 35px;
    border-radius: 35px;

    
}



.back_monkey {
    
    background-image: url('https://media.discordapp.net/attachments/918143224420376608/1030124271965569174/unknown.png');
    background-position: center;
    background-size: cover;

    height: 100%;
    position: fixed;
    top: 0;
    text-align: center;
}

.corpo_monkey {
    position: absolute;
    left: 50%;
}

@media ( max-width: 768px ) {
    .back_monkey {
        position: absolute;
        height: 100%;
    }
    .corpo_monkey {
        top: 100%;
        left: 0%;
    }
}

.logo_img {
    width: 100%;
  
    top: 10%;
}



